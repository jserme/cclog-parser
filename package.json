{
  "name": "cclog-parser",
  "version": "2.0.0",
  "description": "Modern conventional-changelog parser with ES modules and TypeScript support",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\" \"*.{md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\" \"*.{md,json}\"",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "commit": "git-cz",
    "release": "changeset publish",
    "changeset": "changeset",
    "version": "changeset version"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jserme/cclog-parser.git"
  },
  "keywords": [
    "conventional-changelog",
    "changelog",
    "parser",
    "conventional-commits",
    "semantic-release",
    "typescript",
    "esm"
  ],
  "author": "jserme <dev.hubo@gmail.com> (http://jser.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jserme/cclog-parser/issues"
  },
  "homepage": "https://github.com/jserme/cclog-parser#readme",
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "^3.2.4",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
